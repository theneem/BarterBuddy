<!DOCTYPE html>
@{
    @Html.Partial("_BlankLayout")
}
<html>
<head>
   
</head>
<body>
    <div id="loadingDisplay" class="progressBarLoad">
        <h2>Loading...</h2>
        <div class="progress progress-striped ">
            <div id="loadProgressBar" class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 20%"></div>
        </div>
    </div>
    <div class="headerMain">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6 col-sm-6 col-xs-12 logoMain">
                    <a href="@Url.Action("~/Login/Login")" title="Barter Buddy"><img src="@Url.Content("~/images/logo.png")" /></a>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-12 LoginRight ">
                    <div class="menuMain">
                        <div class="menuIcon">
                            <span class="oneS"></span>
                            <span class="twoS"></span>
                            <span class="treS"></span>
                        </div>
                        <div class="menuHere" style="text-align:right">
                            @Html.Partial("DraggebleMenu");
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@
</body>
</html>
